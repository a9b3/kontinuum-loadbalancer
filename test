














upstream esayemm-styleguide {
  least_conn;
  
  server /registry/minions/159.203.237.2:31256;
  
  server /registry/minions/192.241.194.169:31256;
  
  server /registry/minions/159.203.229.250:31256;
  
}

server {
  listen 80;

  
  server_name esayemm-styleguide.foo.com;
  

  location / {
    proxy_pass http://esayemm-styleguide;
    proxy_redirect off;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded_For $proxy_add_x_forwarded_for;
  }
}


















